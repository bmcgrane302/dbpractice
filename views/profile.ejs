<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1><%= com.name %></h1>
    <p><%= com.description %></p>
    <p>Suite-<%= com.suite %></p>

    <h3>Company Members</h3>

    <% for(let i = 0; i < members.length; i++){ %>
      <p><%= members[i].first_name %> <%=  members[i].last_name %></p>
      <p><%= members[i].email %></p>
      <p><%= members[i].position %></p>

    <% } %>

    <form  action="/member/<%= com.id %>" method="post">
      <input type="text" name="first_name" placeholder="first name">
      <input type="text" name="last_name" placeholder="last name">
      <input type="text" name="email" placeholder="email">
      <input type="text" name="position" placeholder="position">
       <input type="submit">
    </form>
  </body>
</html>
